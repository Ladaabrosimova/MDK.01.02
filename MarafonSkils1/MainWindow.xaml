<Window x:Class="MarafonSkils1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarafonSkils1"
        mc:Ignorable="d"
        Title="MarathonSkills" Style="{DynamicResource base_window}" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" >
    <Grid Loaded="Window_Loaded">
        <Rectangle Fill="{DynamicResource dark_grey}" Height="66" VerticalAlignment="Top" Margin="0,418,0,0" />
        <Label x:Name="label_timer" Foreground="White" Content="{Binding Time, UpdateSourceTrigger=PropertyChanged, Mode=OneWay}" HorizontalContentAlignment="Center" Margin="0,423,0,0" VerticalContentAlignment="Center" Width="646" HorizontalAlignment="Center"/>
        <Frame Content="Frame" Height="418" VerticalAlignment="Top" HorizontalAlignment="Center" Width="700" Source="/MainPage.xaml" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
